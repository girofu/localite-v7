{
  "compilerOptions": {
    "target": "es2017",
    "lib": [
      "es2017",
      "es2018",
      "es2019"
    ],
    "module": "esnext",
    "moduleResolution": "bundler",
    "downlevelIteration": true,
    "skipLibCheck": true,
    "strict": false,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "isolatedModules": true,
    "noEmit": true,
    "resolveJsonModule": true
  },
  "extends": "expo/tsconfig.base",
  "include": [
    "**/*.ts",
    "**/*.tsx"
  ],
  "exclude": [
    "node_modules"
  ]
}
