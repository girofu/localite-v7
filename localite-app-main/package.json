{
  "name": "@localite/mobile",
  "version": "1.0.0",
  "main": "expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "type-check": "tsc --noEmit",
    "build:android": "eas build --platform android --profile production",
    "build:ios": "eas build --platform ios --profile production",
    "build:ios:dev": "eas build --platform ios --profile ios-device",
    "build:ios:sim": "eas build --platform ios --profile development",
    "build:all": "eas build --platform all --profile production",
    "build:preview": "eas build --platform all --profile preview",
    "build:development": "eas build --platform all --profile development",
    "submit:android": "eas submit --platform android",
    "submit:ios": "eas submit --platform ios",
    "submit:all": "eas submit --platform all",
    "deploy": "npm run pre-release-check && eas build --platform all --auto-submit",
    "pre-release-check": "node scripts/pre-release-check.cjs",
    "performance-test": "node scripts/performance-test.js",
    "staging:setup": "node scripts/staging-setup.js",
    "env:check": "node -e \"console.log(process.env.NODE_ENV || 'development')\"",
    "prebuild": "expo prebuild",
    "prebuild:clean": "expo prebuild --clean"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@google-cloud/firestore": "^7.11.3",
    "@google-cloud/text-to-speech": "^6.2.0",
    "@google/genai": "^1.15.0",
    "@react-navigation/bottom-tabs": "^6.5.20",
    "@react-navigation/native": "^6.1.17",
    "@react-navigation/stack": "^6.3.29",
    "expo": "~51.0.21",
    "expo-camera": "~15.0.14",
    "expo-google-app-auth": "~8.3.0",
    "expo-image-picker": "~15.1.0",
    "expo-linear-gradient": "~13.0.2",
    "expo-location": "~17.0.1",
    "firebase": "^10.12.2",
    "geolib": "^3.3.4",
    "react": "18.2.0",
    "react-native": "0.74.5",
    "react-native-gesture-handler": "~2.16.1",
    "react-native-maps": "1.14.0",
    "react-native-reanimated": "~3.10.1",
    "react-native-safe-area-context": "4.10.5",
    "react-native-screens": "3.31.1"
  },
  "devDependencies": {
    "@babel/core": "^7.24.0",
    "@babel/preset-env": "^7.28.3",
    "@babel/preset-typescript": "^7.27.1",
    "@eslint/js": "^9.34.0",
    "@expo/cli": "^0.24.21",
    "@react-native-community/cli": "^20.0.1",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^13.3.2",
    "@types/jest": "^30.0.0",
    "@types/react": "~18.2.79",
    "@typescript-eslint/eslint-plugin": "^8.40.0",
    "@typescript-eslint/parser": "^8.40.0",
    "babel-jest": "~29.7.0",
    "babel-preset-expo": "~11.0.0",
    "eslint": "^8.57.0",
    "eslint-config-expo": "^7.0.0",
    "eslint-plugin-react-native": "^4.1.0",
    "jest": "~29.7.0",
    "jest-environment-jsdom": "~29.7.0",
    "react-test-renderer": "18.2.0",
    "ts-jest": "^29.1.2",
    "typescript": "~5.3.3"
  }
}
